<h1>Editing reservation</h1>

<%= semantic_form_for @reservation do |f| %>
  <%= f.inputs do %>
    <%= f.input :feedback %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>


<%= link_to 'Show', @reservation %> |
<%= link_to 'Back', reservations_path %>
